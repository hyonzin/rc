alias l="ls -CF"
alias ll="ls -alF"
alias la="ls -aF"

# Set CLICOLOR if you want Ansi Colors in iTerm2
export CLICOLOR=1

# Set colors to match iTerm2 Terminal Colors
export TERM=xterm-256color

# tail
alias tf="tail -F"

#for PS1
export PS1="\[\033[31m\]T\t \[\033[32m\]\w\n\[\033[33m\]\u@MacBook \$ \e[0m\]"
